package kr.or.ddit.mvc.dao;

import java.util.List;

import kr.or.ddit.mvc.vo.BoardVO;

public interface IBoardDao {
	public int insertBoard(BoardVO boardVo);
	public int deleteBoard(int boardO);
	public int updateBoard(int boardNo);
	public List<BoardVO> getAllBoard(BoardVO boardVo);
	public List<BoardVO> getSelectBoard(BoardVO boardVo);
	public List<BoardVO> getSearchBoard(BoardVO boardVo);
	public List<BoardVO> getReadBoard(BoardVO boardVo);
}
